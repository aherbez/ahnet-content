package  {	import flash.display.MovieClip;		public class QuakeIMG extends MovieClip	{		public var mag:Number;		private var x_pos:Number;		private var y_pos:Number;				private var  map_width:int = 800;		private var map_height:int = 400;		public function QuakeIMG(x_in:Number,y_in:Number,mag_in:Number) 		{			// constructor code			this.mag = mag_in;			this.x_pos = x_in;			this.y_pos = y_in;						this.x = this.x_pos * map_width;			this.y = this.y_pos * map_height;						this.scaleX = this.mag/8.0 * 2.8 + 0.2;			this.scaleY = this.mag/8.0 * 2.8 + 0.2;						this.alpha = this.mag/8.0 * 0.7 + 0.3;						// trace(this.x, this.y, this.mag);		}	}	}