package  {	import GameObject;	public class BigEnemy extends Enemy	{		public function BigEnemy(p:Player) 		{			// constructor code			super(p);			this.mySpeed = 2;			this.attack = 50;						this.x = 300;			this.y = 300;		}		override protected function destroy()		{			Game.instance.WinGame();			super.destroy();		}		override protected function collideWithPlayer()		{			// trace('big dude!');			if (this.playerRef.attackStr < 10)			{				this.playerRef.damage(this.attack);			}			else			{				this.playerRef.damage(this.attack);				this.damage(this.playerRef.attackStr);							}						/*			if (this.playerRef.isDead() == false)			{								if (this.playerRef.attackStr < 10)				{					this.playerRef.damage(this.attack);				}				else				{					this.playerRef.damage(this.				}						}			*/		}	}	}